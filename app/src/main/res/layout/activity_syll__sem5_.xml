<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Syll_Sem5_Activity">

    <Button
        android:id="@+id/button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="38dp"
        android:layout_marginRight="38dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="80dp"
        android:background="@drawable/input_design"
        android:text="Database Management Systems"
        android:fontFamily="Courier"
        android:textSize="18sp"
        />

    <Button
        android:id="@+id/button1"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/button"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="38dp"
        android:layout_marginRight="38dp"
        android:background="@drawable/input_design"
        android:text="Formal Language And Automata Theory"
        android:fontFamily="Courier"
        android:textSize="18sp"
        />

    <Button
        android:id="@+id/button2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button1"
        android:layout_alignParentStart="true"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="38dp"
        android:layout_marginRight="38dp"
        android:layout_centerVertical="true"
        android:background="@drawable/input_design"
        android:text="Artifical Intelligence"
        android:fontFamily="Courier"
        android:textSize="18sp"/>

    <Button
        android:id="@+id/button3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/button2"
        android:background="@drawable/input_design"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="38dp"
        android:layout_marginRight="38dp"
        android:text="Software Engineering"
        android:fontFamily="Courier"
        android:textSize="18sp"
        />

    <Button
        android:id="@+id/button4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button3"
        android:background="@drawable/input_design"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="38dp"
        android:layout_marginRight="38dp"
        android:text="Professional skill Development"
        android:fontFamily="Courier"
        android:textSize="18sp"/>

    <Button
        android:id="@+id/button5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="38dp"
        android:layout_marginRight="38dp"
        android:layout_below="@+id/button4"
        android:background="@drawable/input_design"
        android:fontFamily="Courier"
        android:textSize="18sp"
        android:text="Constition of India-Basic feature and Fundamental Principles" />

    <Button
        android:id="@+id/button6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="38dp"
        android:layout_marginRight="38dp"
        android:layout_below="@+id/button5"
        android:background="@drawable/input_design"
        android:fontFamily="Courier"
        android:textSize="18sp"
        android:text="MOOCs/SWAYAM/NPTEL etc. Courses-1" />

    <Button
        android:id="@+id/button7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="38dp"
        android:layout_marginRight="38dp"
        android:layout_below="@+id/button6"
        android:background="@drawable/input_design"
        android:fontFamily="Courier"
        android:textSize="18sp"
        android:text="Summer Industry Internship-2" />

    <Button
        android:id="@+id/button8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="38dp"
        android:layout_marginRight="38dp"
        android:layout_below="@+id/button7"
        android:background="@drawable/input_design"
        android:fontFamily="Courier"
        android:textSize="18sp"
        android:text="Seminar" />


</RelativeLayout>