<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Syll_Sem5_Civil_Activity">
    <Button
        android:id="@+id/button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="38dp"
        android:layout_marginRight="38dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="80dp"
        android:background="@drawable/input_design"
        android:text="Mechanics of material"
        android:fontFamily="Courier"
        android:textSize="18sp"
        />

    <Button
        android:id="@+id/button1"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/button"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="38dp"
        android:layout_marginRight="38dp"
        android:background="@drawable/input_design"
        android:text="hydraulic engineering"
        android:fontFamily="Courier"
        android:textSize="18sp"
        />

    <Button
        android:id="@+id/button2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button1"
        android:layout_alignParentStart="true"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="38dp"
        android:layout_marginRight="38dp"
        android:layout_centerVertical="true"
        android:background="@drawable/input_design"
        android:text="analysis and design of concrete structure"
        android:fontFamily="Courier"
        android:textSize="18sp"/>

    <Button
        android:id="@+id/button3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/button2"
        android:background="@drawable/input_design"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="38dp"
        android:layout_marginRight="38dp"
        android:text="geotechnical engineering-1"
        android:fontFamily="Courier"
        android:textSize="18sp"
        />

    <Button
        android:id="@+id/button4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button3"
        android:background="@drawable/input_design"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="38dp"
        android:layout_marginRight="38dp"
        android:text="hydrology and water resources engineering"
        android:fontFamily="Courier"
        android:textSize="18sp"/>

    <Button
        android:id="@+id/button5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="38dp"
        android:layout_marginRight="38dp"
        android:layout_below="@+id/button4"
        android:background="@drawable/input_design"
        android:fontFamily="Courier"
        android:textSize="18sp"
        android:text="environmental engineering " />

    <Button
        android:id="@+id/button6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="38dp"
        android:layout_marginRight="38dp"
        android:layout_below="@+id/button5"
        android:background="@drawable/input_design"
        android:fontFamily="Courier"
        android:textSize="18sp"
        android:text="transportation engineering" />

    <Button

    android:id="@+id/button7"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_alignParentStart="true"
    android:layout_marginTop="5dp"
    android:layout_marginLeft="38dp"
    android:layout_marginRight="38dp"
    android:layout_below="@+id/button6"
    android:background="@drawable/input_design"
    android:fontFamily="Courier"
    android:textSize="18sp"
    android:text="Constition of india/Esence of indian traditional knowledge" />

    <Button
        android:id="@+id/button8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="38dp"
        android:layout_marginRight="38dp"
        android:layout_below="@+id/button7"
        android:background="@drawable/input_design"
        android:fontFamily="Courier"
        android:textSize="18sp"
        android:text="innovation/ipr/Entrepreneurship/internship at end of 2nd emester for 6 week" />



</RelativeLayout>